<animation>
  <type>rotate</type>
  <object-name>needle_1</object-name>
  <property>engines/engine[1]/n1</property>
  <interpolation>
    <entry><ind>  0.0</ind>	<dep>  47</dep></entry>
    <entry><ind> 10.0</ind>	<dep>  80</dep></entry>
    <entry><ind> 20.0</ind>	<dep> 110</dep></entry>
    <entry><ind> 30.0</ind>	<dep> 142</dep></entry>
    <entry><ind> 40.0</ind>	<dep> 173</dep></entry>
    <entry><ind> 50.0</ind>	<dep> 204</dep></entry>
    <entry><ind> 60.0</ind>	<dep> 235</dep></entry>
    <entry><ind> 65.0</ind>	<dep> 250</dep></entry>
    <entry><ind> 70.0</ind>	<dep> 266</dep></entry>
    <entry><ind> 75.0</ind>	<dep> 282</dep></entry>
    <entry><ind> 80.0</ind>	<dep> 298</dep></entry>
    <entry><ind> 84.0</ind>	<dep> 310</dep></entry>
    <entry><ind> 90.0</ind>	<dep> 328</dep></entry>
    <entry><ind> 94.0</ind>	<dep> 341</dep></entry>
