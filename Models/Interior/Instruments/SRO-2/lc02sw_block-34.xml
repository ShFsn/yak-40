<?xml version="1.0"?>

<!-- Block No. 34 of Komplex 020M, another switch... -->

<PropertyList>
 <path>../../shared/switches/vng-15.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("yak-40/SRO-2/block-34_spare-working", 1.0 );
      </script>
    </binding>
<!-- animation, begin with sw -->
    <binding>
      <command>nasal</command>
      <script>
	interpolate("yak-40/SRO-2/sw_block-34_spare-working", 1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>block34spare</tooltip-id>
      <label>Block 34
SPARE</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("yak-40/SRO-2/block-34_spare-working", 0.0 );
      </script>
    </binding>
<!-- animation, begin with sw -->
    <binding>
      <command>nasal</command>
      <script>
	interpolate("yak-40/SRO-2/sw_block-34_spare-working", 0.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>block34working</tooltip-id>
      <label>Block 34
WORKING</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>yak-40/SRO-2/sw_block-34_spare-working</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
    <entry><ind>0.0</ind>	<dep>-30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
