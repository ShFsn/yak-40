    <entry><ind>100.0</ind>	<dep> 360.0</dep></entry>
    <entry><ind>110.0</ind>	<dep> 391.5</dep></entry>
  </interpolation>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>

