<?xml version="1.0"?>

<!-- PPG-15k, Main (selected) or Reserve (standby) frequency of ARK-9 No.1 used for MP-2 -->

<PropertyList>
 <path>../../shared/switches/png-15k.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("yak-40/ARK-9/sw_freq_main1-rsrv1", 1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>arkmainrsrv1m</tooltip-id>
      <label>ARK No.1
    MAIN
frequency</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
	interpolate("yak-40/ARK-9/sw_freq_main1-rsrv1", 0.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>arkmainrsrv1r</tooltip-id>
      <label>ARK No.1
RESERVE
frequency</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>yak-40/ARK-9/sw_freq_main1-rsrv1</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
    <entry><ind>0.0</ind>	<dep>-30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
