<?xml version="1.0"?>

<!-- Block No. 34 of Komplex 020M -->

<PropertyList>
 <path>../../shared/switches/vng-15.ac</path>

 <animation>
  <type>pick</type>
  <object-name>clickspot_up</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("yak-40/SRO-2/block-34", 1 );
      </script>
    </binding>
<!-- animation, begin with sw -->
    <binding>
      <command>nasal</command>
      <script>
	interpolate("yak-40/SRO-2/sw_block-34", 1.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>block34_1</tooltip-id>
      <label>Block 34
SETTING 1</label>
    </binding>
   </hovered>
 </animation>

 <animation>
  <type>pick</type>
  <object-name>clickspot_down</object-name>
  <visible>false</visible>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>nasal</command>
      <script>
        setprop("yak-40/SRO-2/block-34", 2 );
      </script>
    </binding>
<!-- animation, begin with sw -->
    <binding>
      <command>nasal</command>
      <script>
	interpolate("yak-40/SRO-2/sw_block-34", 0.0, 0.1 );
      </script>
    </binding>
  </action>
   <hovered>
    <binding>
      <command>set-tooltip</command>
      <tooltip-id>block34_2</tooltip-id>
      <label>Block 34
SETTING 2</label>
    </binding>
   </hovered>
 </animation>

 <animation>
   <type>rotate</type>
   <object-name>lever</object-name>
   <property>yak-40/SRO-2/sw_block-34</property>
  <interpolation>
    <entry><ind>1.0</ind>	<dep>30.0</dep></entry>
    <entry><ind>0.0</ind>	<dep>-30.0</dep></entry>
  </interpolation>
  <axis>
    <x>0.0</x>
    <y>-1.0</y>
    <z>0.0</z>
  </axis>
 </animation>

</PropertyList>
