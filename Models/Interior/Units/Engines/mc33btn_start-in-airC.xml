<?xml version="1.0"?>

<!-- 2KNR, Center Engine Start in Air -->

<PropertyList>
 <path>../../shared/buttons/2knr.ac</path>

 <animation>
  <type>pick</type>
  <object-name>press_me</object-name>
  <visible>true</visible>
  <action>
   <button>0</button>
   <repeatable>true</repeatable>
   <binding>
     <command>nasal</command>
     <script>
	interpolate("yak-40/Engines/start-in-airC", 1.0, 0.1 );
     </script>
   </binding>
  <mod-up>
   <binding>
     <command>nasal</command>
     <script>
	interpolate("yak-40/Engines/start-in-airC", 0.0, 0.1 );
     </script>
   </binding>
  </mod-up>
  </action>
 </animation>

 <animation>
  <type>translate</type>
  <object-name>press_me</object-name>
  <property>yak-40/Engines/start-in-airC</property>
  <factor>0.004</factor>
  <axis>
    <x>-1</x>
    <y>0</y>
    <z>0</z>
  </axis>
 </animation>
</PropertyList>
